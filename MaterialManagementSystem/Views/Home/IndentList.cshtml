        <div class="page-content-wrapper">
            <div class="page-content">
                



               

                    <div class="col-md-10">

                        <img src="~/assets/CCE_Header_Image (1).jpg" style="max-width:100%;min-width:100%" vertical-align:top" />

                    </div>

                    <div style="float:right;" class="col-md-2 sidenav not-printable">
                        <a href="javascript:;" class="icon-btn" style="background-color:#45b6af">
                            <i class="fa fa-group"></i>
                            <b style="color:white">    @ViewBag.done</b>
                            <div>
                                <b>Total Done</b>
                            </div>
                            <span class="badge badge-success">
                                <b style="color:white">    @ViewBag.done</b>
                            </span>
                        </a><br />


                        <a href="javascript:;" class="icon-btn" style="background-color:#89C4F4">
                            <i class="fa fa-sitemap"></i>
                            <b style="color:white">     @ViewBag.pending</b>
                            <div>
                                <b>   Total Pending</b>
                            </div>
                            <span class="badge badge-info">
                                <b>     @ViewBag.pending</b>
                            </span>
                        </a><br />


                        <a href="javascript:;" class="icon-btn" style="background-color:#f3565d">
                            <i class="fa fa-bar-chart-o"></i>
                            <b style="color:white">   @ViewBag.cancle</b>
                            <div>
                                <b>  Total Cancle</b>
                            </div>
                            <span class="badge badge-danger">
                                <b>   @ViewBag.cancle</b>
                            </span>
                        </a><br />


                        <a href="javascript:;" class="icon-btn" style="background-color:#428bca">
                            <i class="fa fa-globe"></i>
                            <b style="color:white">   @ViewBag.total</b>
                            <div>
                                <b>Total Indent</b>
                            </div>
                            <span class="badge badge-primary">
                                <b>   @ViewBag.total</b>
                            </span>
                        </a>
                    </div>

               
                
               
                  

               
                 <br /><br />
                
                    <div class="col-md-10" >
                        <!-- BEGIN EXAMPLE TABLE PORTLET-->
                        <div class="portlet box grey-cascade">
                            <div class="portlet-title">
                                <div class="caption">
                                    <i class="fa fa-globe"></i>Indent
                                </div>
                               
                            </div>
                            <div class="portlet-body">
                               
                                <div class="table-toolbar" >
                                    <div class="row" >
                                        <div class="col-md-6">
                                            <div class="btn-group">
                                                @*<button id="sample_editable_1_new" class="btn green">
                                                    Add New <i class="fa fa-plus"></i>
                                                </button>
                                                <a href="/Requisition/Home/Create" class="btn green">
                                                    Create New
                                                    <i class="fa fa-plus"></i>
                                                </a>*@
                                                <button class="btn green not-printable" onclick="location.href='@Url.Action("Create", "Home")'"> Add New <i class="fa fa-plus"></i></button>

                                            </div>
                                        </div>
                                       
                                    </div>
                                </div>
                                @if (!Model.Any())
                                {
                                     <div style="color:red"><marquee><b>*No Records Found*</b></marquee></div>
                                }
                                <table class="table table-striped table-bordered table-hover" style="white-space:nowrap">
                                  
                                    <thead>
                                       
                                        <tr style="text-align:center">    
                                          
                                            <th>
                                                @Html.DisplayNameFor(model => model.IndentDate)
                                               
                                            </th>
                                            <th style="text-align:center">
                                                @Html.DisplayNameFor(model => model.Item)
                                               
                                               
                                            </th>

                                            @*<th>
                                                @Html.DisplayNameFor(model => model.Description)
                                            </th>*@
                                            @*<th>
                                                @Html.DisplayNameFor(model => model.ItemCode)
                                            </th>*@
                                            <th>
                                                @Html.DisplayNameFor(model => model.IndentNo)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Quantity)
                                            </th>
                                            @*<th>
                                                @Html.DisplayNameFor(model => model.SourceFund)
                                            </th>*@
                                            <th>
                                                @Html.DisplayNameFor(model => model.ReceivedDate)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Status)
                                            </th>
                                         
                                            @*<th>
                                                @Html.DisplayNameFor(model => model.IsActive)
                                            </th>*@
                                            <th></th>
                                        </tr>
                                    </thead>
                                    @foreach (var item in Model)
                                    {
                                        <tbody style="white-space:nowrap">
                                           
                                            <tr>
                                                
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.IndentDate)
                                                </td>
                                               
                                                 <td>
                                                  
                                                        @*@Html.DisplayFor(modelItem => item.Item)*@
                                                 <ul>
                                                     <li>
                                                         @Html.Raw(item.Item.Replace(",", "<br><li>"))
                                                     </li>
                                                 </ul>   
                                                  
                                                         


                                                  </td >
                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.Description)
                                                </td>*@
                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.ItemCode)
                                                </td>*@
                                                    <td >
                                                        @Html.DisplayFor(modelItem => item.IndentNo)
                                                    </td >


                                                
                                                    <td style="text-align:center">
                                                        @Html.Raw(item.Quantity.Replace(",", "<br>"))

                                                        @*<ul>
                <li>
                    @Html.Raw(item.Quantity.Replace(",", "<br><li>"))
                 </li>
                </ul>*@

                                                    </td>
                                              
                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.SourceFund)
                                                </td>*@
                                                @if (item.ReceivedDate==null)
	{
		 <td><B style="color:red">Not RECEIVE</B></td>          }
                                                else
                                                {
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.ReceivedDate)
                                                    </td>
                                                }
                                                   


                                                    <td >
                                                        @if (item.Status == "Done")
                                                        {
                                                        <span class="label label-sm label-success" style="font-size:14px">
                                                            Done
                                                        </span>
                                                        }
                                                    @if (item.Status == "Pending")
                                                    {
                                                        <span class="label label-sm label-info" style="font-size:14px">
                                                            Pending
                                                        </span>

                                                    }
                                                    @if (item.Status == "Cancle")
                                                    {
                                                        <span class="label label-sm label-danger" style="font-size:14px">
                                                            Cancle
                                                        </span>

                                                    }
                                                </td>

                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.IsActive)
                                                </td>*@



                                                <td class="not-printable">


                                                   
                                                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "btn default btn-xs  fa fa-pencil purple-stripe" }) 
                                                    @Html.ActionLink("View", "Details", new { id = item.Id }, new { @class = "btn default btn-xs fa fa-gear yellow-stripe" }) 
                                                    @*<button class="btn btn-sm red filter-cancel"><i class="fa fa-times"></i> Reset</button>*@
                                                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn default btn-xs  fa fa-trash-o red-stripe", onclick = "return confirm('Are you sure you want to delete requisition: " + item.IndentNo + "');" })
                                                </td>
                                                </tr>

</tbody>
                                        }
                                    
                                   
                                </table>
                            </div>
                        </div>
                        <!-- END EXAMPLE TABLE PORTLET-->
                    </div>
               
               
                <!-- END PAGE CONTENT-->
            </div>
        </div>
        <!-- END CONTENT -->
